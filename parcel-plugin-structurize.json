{"parcel-plugin-structurize": {
    "rules": [
        {
            "match": "*.js",
            "folder": "scripts"
        },
        {
            "match": "*.{jpg,png,gif,svg}",
            "folder": "images"
        },
        {
          "match": "*.{css,eot,ttf,woff2,woff}",
          "folder": "css"
      }
    ]
}}